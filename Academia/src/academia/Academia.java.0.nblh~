    private void btnSRegistrarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnSRegistrarActionPerformed
        boolean verificar=true,verificar1=true;
        verificar=VerificarCampos(verificar);
        if(cbxSServicio.getSelectedIndex()==1){
            if(Integer.parseInt(txtSFecha2.getText())>12||Integer.parseInt(txtSFecha2.getText())<1&&Integer.parseInt(txtSFecha1.getText())<1||Integer.parseInt(txtSFecha1.getText())>maximodia(Integer.parseInt(M),Integer.parseInt(A))){
                JOptionPane.showMessageDialog(this,"Ingrese Correctamente la fecha ","ERROR", JOptionPane.ERROR_MESSAGE);
                verificar1=false;
                verificar=false;
            }
        }
        if(verificar){
            if(cbxSServicio.getSelectedIndex()==1){
                 tblSRecientes.setValueAt(cbxSServicio.getSelectedItem(), 0, 0);
                 tblSRecientes.setValueAt("1", 0, 1);
                 tblSRecientes.setValueAt(cbxSHora.getSelectedItem(), 0, 2);
                 tblSRecientes.setValueAt(txtSPrecioH.getText(), 0, 3);
                 tblSRecientes.setValueAt(txtSFecha1.getText()+"/"+txtSFecha2.getText()+"/"+txtSFecha3.getText(), 0, 4);
            }
            JOptionPane.showMessageDialog(this,"Alumno Registrado Satisfactoriamente "+campos,"REGISTRADO", JOptionPane.INFORMATION_MESSAGE);
            limpiarServicio();
          }
        else{
            if(verificar1)
                JOptionPane.showMessageDialog(this,"Campos Vacios: "+campos,"ADVERTENCIA", JOptionPane.WARNING_MESSAGE);
        }
    }//GEN-LAST:event_btnSRegistrarActionPerformed

    private void btnSSeleccionarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnSSeleccionarActionPerformed
        int fila,cbx=-1;
        String horario,fecha,fecha1=null,fecha2=null,fecha3=null;
        fila=tblSRecientes.getSelectedRow();
        if(fila==-1){
            JOptionPane.showMessageDialog(this,"Seleccione una fila", "ADVERTENCIA", JOptionPane.WARNING_MESSAGE);
        }
        else{
            horario=String.valueOf(tblSRecientes.getValueAt(fila,2));
            outerloop4:
            for(int i=0;i<=cbxSHora.getItemCount();i++){
                if(horario.equalsIgnoreCase(String.valueOf(cbxSHora.getItemAt(i)))){
                    cbx=i;
                    break outerloop4;
                }
            }
            cbxSHora.setSelectedIndex(cbx);
            txtSPrecioH.setText(String.valueOf(tblSRecientes.getValueAt(fila,3)));
            fecha=String.valueOf(tblSRecientes.getValueAt(fila, 4));
            int contador=0,aux=0;
            for(int j=0;j<fecha.length();j++){
                if(fecha.charAt(j)=='/'){
                    contador++;
                    if(contador==1){ 
                        fecha1=fecha.substring(0, j);
                        aux=j+1;
                    }
                    if(contador==2){
                        fecha2=fecha.substring(aux,j);
                        aux=j+1;
                        fecha3=fecha.substring(aux, fecha.length());
                    }
                }
            }
            txtSFecha1.setText(fecha1);
            txtSFecha2.setText(fecha2);
            txtSFecha3.setText(fecha3);
        }
    }//GEN-LAST:event_btnSSeleccionarActionPerformed

    private void txtSFecha3KeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_txtSFecha3KeyTyped
        char caracter=evt.getKeyChar();
        String campo;
        if(caracter<'0'||caracter>'9'){
            if(caracter!=evt.VK_BACK_SPACE)
                evt.consume();
        }
        campo=txtSFecha3.getText();
        if(campo.length()==4){
            evt.consume();
        }
    }//GEN-LAST:event_txtSFecha3KeyTyped

    private void txtSFecha2KeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_txtSFecha2KeyTyped
        char caracter=evt.getKeyChar();
        String campo=txtSFecha2.getText();
        if(caracter<'0'||caracter>'9'){
            if(caracter!=evt.VK_BACK_SPACE)
                evt.consume();
        }
        if(campo.length()==2){
            evt.consume();
        }
    }//GEN-LAST:event_txtSFecha2KeyTyped

    private void txtSFecha1KeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_txtSFecha1KeyTyped
        char caracter=evt.getKeyChar();
        String campo=txtSFecha1.getText(); 
        if(caracter<'0'||caracter>'9'){
            if(caracter!=evt.VK_BACK_SPACE)
                evt.consume();
        }
        if(campo.length()==2){
            evt.consume();
        }
    }//GEN-LAST:event_txtSFecha1KeyTyped

    private void btnIngresosBActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnIngresosBActionPerformed
        HSSFWorkbook libro = new HSSFWorkbook();
        HSSFSheet hoja = libro.createSheet();
        HSSFRow fila = hoja.createRow(0);
        HSSFCell celda = fila.createCell((short)0);
        HSSFRichTextString texto = new HSSFRichTextString("hola mundo");
        celda.setCellValue(texto);
    }//GEN-LAST:event_btnIngresosBActionPerformed

    public boolean VerificarCampos(boolean verificar){
        campos="";
        if(cbxSServicio.getSelectedIndex()==0){
            verificar=verificar&&false;
            campos="Servicio, ";
        }
        if(cbxSServicio.getSelectedIndex()==2){
            if(cbxSTipo.getSelectedIndex()==0){
                verificar=verificar&&false;
                campos+="Tipo de Clases Particulares, ";
            }
            verificar=verificar&&recorrer(txtSHoraI.getText());
            if(recorrer(txtSHoraI.getText())==false){
                campos+="Hora de Inicio, ";
            }
            verificar=verificar&&recorrer(txtSHoraT.getText());
            if(recorrer(txtSHoraT.getText())==false){
                campos+="Hora de Termino, ";
            }
        }
        if(cbxSServicio.getSelectedIndex()==3){
            verificar=verificar&&recorrer(txtSEntrega.getText());
            if(recorrer(txtSEntrega.getText())==false){
                verificar=verificar&&false;
                campos+="Fecha de Entrega, ";
            }
        }
        if(cbxSServicio.getSelectedIndex()==1){
            if(cbxSHora.getSelectedIndex()==0){
                verificar=verificar&&false;
                campos+="Horario, ";
            }
            verificar=verificar&&recorrer(txtSFecha1.getText());
            verificar=verificar&&recorrer(txtSFecha2.getText());
            verificar=verificar&&recorrer(txtSFecha3.getText());
            if(recorrer(txtSFecha3.getText())==false&&recorrer(txtSFecha2.getText())==false&&recorrer(txtSFecha1.getText())==false){
                campos+="Hora de Termino, ";
            }
        }
        verificar=verificar&&recorrer(txtSNombres.getText());
        if(recorrer(txtSNombres.getText())==false){
                campos+="Nombres, ";
        }
        verificar=verificar&&recorrer(txtSApellidoPat.getText());
        if(recorrer(txtSApellidoPat.getText())==false){
                campos+="Apellido Paterno, ";
        }
        verificar=verificar&&recorrer(txtSApellidoMat.getText());
        if(recorrer(txtSApellidoMat.getText())==false){
                campos+="Apellido Materno, ";
        }
        verificar=verificar&&recorrer(txtSDireccion1.getText());
        if(recorrer(txtSDireccion1.getText())==false){
                campos+="Direccion, ";
        }
        verificar=verificar&&recorrer(txtSPrecioH.getText());
        if(recorrer(txtSPrecioH.getText())==false){
                campos+="Precio por Hora, ";
        }
        return verificar;
    }
    public boolean recorrer(String c){
        boolean v=true;
        int contador=0;
        if(c.length()==0){
            v=false;
        }
        else{
            for(int i=0;i<c.length();i++){
                if(c.charAt(i)==' '){
                    contador++;
                }
            }
            if(contador==c.length()){
                v=false;
            }
        }
        return v;
    }
    public void CambiodeItem(){
        switch(cbxSServicio.getSelectedIndex()){
            case 0:lblSHoraI.setVisible(false);
                   lblSHoraT.setVisible(false);
                   lblSHorario.setVisible(false);
                   txtSHoraI.setVisible(false);
                   txtSHoraT.setVisible(false);
                   cbxSHora.setVisible(false);
                   cbxSTipo.setVisible(false);
                   lblSFecha.setVisible(false);
                   txtSFecha1.setVisible(false);
                   txtSFecha2.setVisible(false);
                   txtSFecha3.setVisible(false);
                   lblextra1.setVisible(false);
                   lblextra2.setVisible(false);
                   lblSEntrega.setVisible(false);
                   txtSEntrega.setVisible(false);
                   tblSRecientes.setVisible(false);
                   scrSRecientes.setVisible(false);
                   btnSSeleccionar.setVisible(false);
                break;
            case 1:lblSHoraI.setVisible(false);
                   lblSHoraT.setVisible(false);
                   lblSHorario.setVisible(true);
                   txtSHoraI.setVisible(false);
                   txtSHoraT.setVisible(false);
                   cbxSHora.setVisible(true);
                   cbxSTipo.setVisible(false);
                   lblSFecha.setVisible(true);
                   txtSFecha1.setVisible(true);
                   txtSFecha2.setVisible(true);
                   txtSFecha3.setVisible(true);
                   lblextra1.setVisible(true);
                   lblextra2.setVisible(true);
                   lblSEntrega.setVisible(false);
                   txtSEntrega.setVisible(false);
                   tblSRecientes.setVisible(true);
                   scrSRecientes.setVisible(true);
                   btnSSeleccionar.setVisible(true);
                   break;
            case 2:lblSHoraI.setVisible(true);
                   lblSHoraT.setVisible(true);
                   lblSHorario.setVisible(false);
                   txtSHoraI.setVisible(true);
                   txtSHoraT.setVisible(true);
                   cbxSHora.setVisible(false);
                   cbxSTipo.setVisible(true);
                   lblSFecha.setVisible(false);
                   txtSFecha1.setVisible(false);
                   txtSFecha2.setVisible(false);
                   txtSFecha3.setVisible(false);
                   lblextra1.setVisible(false);
                   lblextra2.setVisible(false);
                   lblSEntrega.setVisible(false);
                   txtSEntrega.setVisible(false);
                   tblSRecientes.setVisible(false);
                   scrSRecientes.setVisible(false);
                   btnSSeleccionar.setVisible(false);
                   break;
            case 3:lblSHoraI.setVisible(false);
                   lblSHoraT.setVisible(false);
                   lblSHorario.setVisible(false);
                   txtSHoraI.setVisible(false);
                   txtSHoraT.setVisible(false);
                   cbxSHora.setVisible(false);
                   cbxSTipo.setVisible(false);
                   lblSFecha.setVisible(false);
                   txtSFecha1.setVisible(false);
                   txtSFecha2.setVisible(false);
                   txtSFecha3.setVisible(false);
                   lblextra1.setVisible(false);
                   lblextra2.setVisible(false);
                   lblSEntrega.setVisible(true);
                   txtSEntrega.setVisible(true);
                   tblSRecientes.setVisible(false);
                   scrSRecientes.setVisible(false);
                   btnSSeleccionar.setVisible(false);
                   break;
        }
    }
    public void limpiarServicio(){
        txtSHoraI.setText("");
        txtSHoraT.setText("");
        cbxSHora.setSelectedIndex(0);
        cbxSTipo.setSelectedIndex(0);
        txtSFecha1.setText("");
        txtSFecha2.setText("");
        txtSFecha3.setText("");
        txtSEntrega.setText("");
        txtSNombres.setText("");
        txtSApellidoPat.setText("");
        txtSApellidoMat.setText("");
        txtSTelefono.setText("");
        txtSDireccion1.setText("");
        txtSDireccion2.setText("");
        txtSCorreo.setText("");
        txtSPrecioH.setText("");
    }
    public void Asistencia(boolean logico,boolean docente,boolean admi){
        btnAsistenciaAlumnos.setVisible(logico);
        btnAsistenciaDocentes.setVisible(logico);
        btnAsistenciaAdministrativa.setVisible(logico);
        lblAsistencia.setVisible(logico);
        lblAsistenciaAD.setVisible(!logico);
        tblAsistencia.setVisible(!logico);
        ScrollAsistencia.setVisible(!logico);
        txtCodigo.setVisible(!logico);
        lblCodigo.setVisible(!logico);
        txtNombreAsis.setVisible(!logico);
        lblNombreAsis.setVisible(!logico);
        txtApellidoPatAsis.setVisible(!logico);
        lblPreparacionAsis.setVisible(!logico);
        btnRegresar.setVisible(!logico);
        lblSalonAsis.setVisible(!logico);
        txtSalonAsis.setVisible(!logico);
        lblApellidoPatAsis.setVisible(!logico);
        txtApellidoMatAsis.setVisible(!logico);
        lblApellidoMatAsis.setVisible(!logico);
        txtPreparacionAsis.setVisible(!logico);
        lblMes.setVisible(!logico);
        lblModoBusqueda.setVisible(!logico);
        cbxModoBusqueda.setVisible(!logico);
        btnLimpiar.setVisible(!logico);
        btnBusqueda.setVisible(false);
        btnFaltantes.setVisible(false);
        btnAnterior.setVisible(!logico);
        btnSiguiente.setVisible(!logico);
        btnActual.setVisible(!logico);
        if(!logico){
            lblMes.setText(ObtenerMes(Integer.parseInt(M),Integer.parseInt(A)));
        }
        if(admi){
            lblSalonAsis.setVisible(logico);
            txtSalonAsis.setVisible(logico);
        }
        focusBusqueda();
    }
