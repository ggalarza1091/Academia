    private void btnActualActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnActualActionPerformed

        limpiarCalendario();         MesAux = Integer.parseInt(M);         AAux = Integer.parseInt(A);         calendario();     }//GEN-LAST:event_btnActualActionPerformed

    private void btnSiguienteActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnSiguienteActionPerformed

        MesAux++;         if (MesAux == 12) {             MesAux = 0;             AAux++;         }         CalendarioSiguiente();     }//GEN-LAST:event_btnSiguienteActionPerformed

    private void btnAnteriorActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnAnteriorActionPerformed

        MesAux--;         if (MesAux == -1) {             MesAux = 11;             AAux--;         }         CalendarioAnterior();     }//GEN-LAST:event_btnAnteriorActionPerformed

    private void btnBusquedaActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnBusquedaActionPerformed

        if (alumno) {             Asistencia(false, false, false);             lblAsistenciaAD.setText("ASISTENCIA DE ALUMNOS");             lblPreparacionAsis.setText("Tipo de Preparacion:");         }         if (administrativa) {             Asistencia(false, false, true);             lblAsistenciaAD.setText("ASISTENCIA DE ADMINISTRATIVOS");             lblPreparacionAsis.setText("Cargo:");         }         if (docente) {             Asistencia(false, true, false);             lblAsistenciaAD.setText("ASISTENCIA DE DOCENTES");             lblPreparacionAsis.setText("Cargo:");         }         alumno = false;         docente = false;         administrativa = false;     }//GEN-LAST:event_btnBusquedaActionPerformed

